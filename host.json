{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  },
  "extensions": {
    "cosmosDB": {
      "connectionStringSetting": "AzureWebJobsCosmosDBConnectionString"
    },
    "blobs": {
      "maxDegreeOfParallelism": 1,
      "maxBatchSize": 100,
      "pollingInterval": "00:00:10"
    }
  }
}
